---
import features from "../../config/features.json";
---

<h1 class="text-center font-bold text-5xl mt-10">features</h1>
<div class="flex">
  {
    features.map((feature) => (
      <div class="bg-slate-800 p-10 rounded-xl m-1 ">
        <h2>{feature.title}</h2>
        <p>
            {feature.description}
        </p>
      </div>
    ))
  }
</div>
